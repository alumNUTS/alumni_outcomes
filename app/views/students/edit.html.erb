
<h1>Edit your Information</h1>
  <% if @student.errors.any? %>
    <ul>
    <% @student.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
    </ul>
  <% end %>
<%= form_for @student do |form| %>
  <%= render form %>
  <div class="actions">
    <%= form.submit 'Submit', class: "submit_edit" %>
  </div>
<% end %>
