

<h1>Login</h1>
<%= @error %>
<p id="notice"><%= notice %></p>
<%= form_tag '/login' do %>
	<%= label_tag :email %>
  <%= text_field_tag :email %>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <%= submit_tag "Login" %>
<% end %>

<a href="/students/new">Create New Student</a>
