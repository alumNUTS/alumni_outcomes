

<div>
  <%= form.label :name %><br>
  <%= form.text_field :name %>
</div>
<div>
  <%= form.label :email %><br>
  <%= form.email_field :email %>
</div>
<% if @student.id == nil %>
  <%= form.label "password" %><br>
  <%= form.password_field :password_digest %>
<% end %>
</div>
<div>
